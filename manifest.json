{
    "manifest_version": 2,
    "name": "Extension test",
    "content_scripts": [],
    "description": "Premier test d'extensions",
    "version": "14657.0",
    "optional_host_permissions": [
        "http://*/*",
        "https://*/*",
        "storage"
